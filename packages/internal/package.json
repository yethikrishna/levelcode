{
  "name": "@levelcode/internal",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./db": {
      "bun": "./src/db/index.ts",
      "import": "./src/db/index.ts",
      "types": "./src/db/index.ts",
      "default": "./src/db/index.ts"
    },
    "./loops": {
      "bun": "./src/loops/index.ts",
      "import": "./src/loops/index.ts",
      "types": "./src/loops/index.ts",
      "default": "./src/loops/index.ts"
    },
    "./openrouter-ai-sdk": {
      "bun": "./src/openrouter-ai-sdk/index.ts",
      "import": "./src/openrouter-ai-sdk/index.ts",
      "types": "./src/openrouter-ai-sdk/index.ts",
      "default": "./src/openrouter-ai-sdk/index.ts"
    },
    "./env": {
      "react-server": "./src/env.react-server.ts",
      "browser": "./src/env.browser.ts",
      "bun": "./src/env.ts",
      "import": "./src/env.ts",
      "types": "./src/env.ts",
      "default": "./src/env.ts"
    },
    "./*": {
      "bun": "./src/*.ts",
      "import": "./src/*.ts",
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit push --config=./src/db/drizzle.config.ts",
    "db:start": "docker compose -f ./src/db/docker-compose.yml up --wait && bun run db:generate && (timeout 1 || sleep 1) && bun run db:migrate",
    "db:e2e:setup": "bun ./src/db/e2e-setup.ts",
    "db:e2e:down": "docker compose -f ./src/db/docker-compose.e2e.yml down --volumes",
    "db:studio": "drizzle-kit studio --config=./src/db/drizzle.config.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.5"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^3.0.17",
    "@levelcode/common": "workspace:*",
    "drizzle-kit": "0.31.8",
    "drizzle-orm": "0.45.1",
    "loops": "^5.0.1",
    "msw": "^2.12.1",
    "postgres": "^3.4.7",
    "server-only": "0.0.1"
  },
  "devDependencies": {}
}
