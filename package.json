{
  "name": "levelcode",
  "version": "1.0.0",
  "description": "LevelCode - An open-source AI coding agent better than Claude Code. Multi-agent architecture for precise codebase editing.",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "author": {
    "name": "Yethikrishna R",
    "email": "yethikrishnarcvn7a@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yethikrishna/levelcode.git"
  },
  "bugs": {
    "url": "https://github.com/yethikrishna/levelcode/issues"
  },
  "homepage": "https://github.com/yethikrishna/levelcode#readme",
  "keywords": [
    "ai",
    "coding-agent",
    "cli",
    "llm",
    "multi-agent",
    "code-generation",
    "developer-tools",
    "typescript",
    "openrouter",
    "claude",
    "gpt",
    "deepseek"
  ],
  "workspaces": [
    ".agents",
    "common",
    "web",
    "packages/*",
    "scripts",
    "evals",
    "sdk",
    "agents",
    "cli"
  ],
  "scripts": {
    "dev": "bun start-cli",
    "up": "bun scripts/start-services.ts",
    "start-cli": "bun --cwd cli dev",
    "start-db": "bun --cwd packages/internal db:start",
    "start-web": "bun start-db && bun --cwd web dev",
    "start-studio": "bun --cwd packages/internal db:studio",
    "down": "bun scripts/stop-services.ts",
    "ps": "bun scripts/status-services.ts",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "release:cli": "bun run --cwd=cli release",
    "release:sdk": "bun run --cwd=sdk release",
    "clean-ts": "find . -name '*.tsbuildinfo' -type f -delete && find . -name '.next' -type d -exec rm -rf {} + 2>/dev/null || true && find . -name 'node_modules' -type d -exec rm -rf {} + 2>/dev/null || true && bun install",
    "typecheck": "bun scripts/check-env-architecture.ts && bun --filter='*' run typecheck && echo 'âœ… All type checks passed!'",
    "test": "bun --filter='{@levelcode/common,@levelcode/agents,@levelcode/agent-runtime,@levelcode/sdk,@levelcode/web,@levelcode/cli,@levelcode/evals,@levelcode/scripts}' run test",
    "init-worktree": "bun scripts/init-worktree.ts",
    "cleanup-worktree": "bun scripts/cleanup-worktree.ts",
    "generate-tool-definitions": "bun scripts/generate-tool-definitions.ts"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.7.3",
    "canvas": "^3.2.0",
    "gif-encoder-2": "^1.0.5",
    "zod": "^4.2.1"
  },
  "overrides": {
    "baseline-browser-mapping": "^2.9.14",
    "zod": "^4.2.1",
    "signal-exit": "3.0.7"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.12",
    "@types/bun": "^1.3.5",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.12",
    "@types/parse-path": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^6.17",
    "bun-types": "^1.3.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "ignore": "^6.0.2",
    "lodash": "4.17.23",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "ts-pattern": "^5.9.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "engines": {
    "bun": ">=1.3.5"
  },
  "packageManager": "bun@1.3.5"
}
