{
  "name": "@levelcode/cli",
  "version": "0.3.8",
  "description": "LevelCode CLI - Terminal-based AI coding agent that outperforms Claude Code",
  "author": {
    "name": "Yethikrishna R",
    "email": "yethikrishnarcvn7a@gmail.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yethikrishna/levelcode.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/yethikrishna/levelcode/issues"
  },
  "homepage": "https://github.com/yethikrishna/levelcode/tree/master/cli",
  "keywords": [
    "levelcode",
    "ai",
    "coding-agent",
    "cli",
    "terminal",
    "llm",
    "openrouter"
  ],
  "private": false,
  "type": "module",
  "bin": {
    "levelcode": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.tsx",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "bun run prebuild:agents && bun run src/index.tsx --cwd ..",
    "prebuild:agents": "bun run scripts/prebuild-agents.ts",
    "build:binary": "bun ./scripts/build-binary.ts levelcode $npm_package_version",
    "release": "bun run scripts/release.ts",
    "test": "NODE_ENV=production bun test",
    "test:tmux-poc": "bun run src/__tests__/tmux-poc.ts",
    "typecheck": "tsc --noEmit -p ."
  },
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.5"
  },
  "dependencies": {
    "@levelcode/sdk": "^0.0.3",
    "@gravity-ai/api": "^0.1.2",
    "@opentui/core": "0.1.74",
    "@opentui/react": "0.1.74",
    "@tanstack/react-query": "^5.90.12",
    "commander": "^14.0.1",
    "immer": "^10.1.3",
    "jimp": "^1.6.0",
    "open": "^10.1.0",
    "pino": "9.4.0",
    "posthog-node": "^5.8.0",
    "react": "^19.0.0",
    "react-reconciler": "^0.32.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "string-width": "^7.2.0",
    "terminal-image": "^4.1.0",
    "ts-pattern": "^5.9.0",
    "unified": "^11.0.0",
    "yoga-layout": "^3.2.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-reconciler": "^0.32.0",
    "react-dom": "^19.0.0",
    "strip-ansi": "^7.1.2"
  }
}
